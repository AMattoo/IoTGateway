<Window x:Class="Waher.Client.WPF.Dialogs.ParameterDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ParameterDialog" Height="500" Width="500" WindowStartupLocation="CenterOwner">
    <DockPanel Name="DialogPanel">
        <DockPanel Width="160" Height="30" DockPanel.Dock="Bottom">
            <Button Content="OK" Height="23" HorizontalAlignment="Left" Name="OkButton" VerticalAlignment="Center" Width="75" IsDefault="True" 
                    ToolTip="Applies changes and closes the dialog." />
            <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Name="CancelButton" Margin="10,0,0,0" VerticalAlignment="Center" Width="75"
                    ToolTip="Closes the dialog without applying changes." IsCancel="True"/>
        </DockPanel>
        <TabControl Name="Tabs" DockPanel.Dock="Top">
        </TabControl>
    </DockPanel>
</Window>
