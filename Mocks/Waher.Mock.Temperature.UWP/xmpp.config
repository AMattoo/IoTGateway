<?xml version='1.0' encoding='utf-8'?>
<SimpleXmppConfiguration xmlns='http://waher.se/SimpleXmppConfiguration.xsd'>
	<Host>ENTER HOST NAME HERE</Host>
	<Port>5222</Port>
	<Account>ENTER ACCOUNT NAME HERE</Account>
	<Password>ENTER PASSWORD HERE</Password>
	<ThingRegistry>ENTER JID OF THING REGISTRY HERE, IF ANY.</ThingRegistry>
	<Provisioning>ENTER JID OF PROVISIONING SERVER HERE, IF ANY.</Provisioning>
	<Events>ENTER JID OF REMOTE XMPP EVENT SINK HERE, IF ANY.</Events>
	<Sniffer>true</Sniffer>
	<TrustServer>false</TrustServer>
	<AllowCramMD5>true</AllowCramMD5>
	<AllowDigestMD5>true</AllowDigestMD5>
	<AllowPlain>false</AllowPlain>
	<AllowScramSHA1>true</AllowScramSHA1>
	<AllowEncryption>true</AllowEncryption>
	<RequestRosterOnStartup>true</RequestRosterOnStartup>
</SimpleXmppConfiguration>
